                android:name="PARENT_ACTIVITY"
                android:value=".MainActivity"
                />
        </activity>
        <activity
            android:name=".vwProduto"
            android:label="@string/title_activity_vw_produto">
            <!--android:parentActivityName=".MainActivity">
            <meta-data
               android:name="android.support.v7" ></meta-data>-->
            </activity>

        <activity
            android:name=".VW_Fornecedor"
            android:label="@string/title_activity_vw_novo_fornecedor"
            android:parentActivityName=".ManterFornecedorActivity" >
            <meta-data
                android:name="PARENT_ACTIVITY"
                android:value=".ManterFornecedorActivity"
            />
        </activity>
        <activity
            android:name=".VWConsultaFornecedorActivity"
            android:label="@string/title_activity_vw_consulta_fornecedor"
            android:parentActivityName=".ManterFornecedorActivity">
            <meta-data
                android:name="PARENT_ACTIVITY"
                android:value=".ManterFornecedorActivity"
                />
        </activity>

    </application>

</manifest>
