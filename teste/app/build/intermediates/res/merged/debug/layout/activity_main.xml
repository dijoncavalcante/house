<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent" android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin"
    tools:context=".MainActivity">


    <TextView
        android:text="Aplicativo em fase de TESTE"
        android:id="@+id/braga"
        android:layout_width="wrap_content"
        android:layout_height="15dp"
        android:layout_centerHorizontal="true"
        />

    <Button
        android:layout_width="150dp"
        android:layout_height="50dp"
        android:id="@+id/manter_produto"
        android:text="@string/manter_produto"
        android:textStyle="bold"
        android:layout_below="@+id/braga"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="60dp"
        />

    <Button
        android:layout_width="150dp"
        android:layout_height="50dp"
        android:id="@+id/estoque"
        android:text="@string/estoque"
        android:textStyle="bold"
        android:layout_below="@+id/manter_produto"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="30dp"/>

    <Button
        android:layout_width="150dp"
        android:layout_height="50dp"
        android:id="@+id/listaDeCompras"
        android:text="@string/lista_de_compras"
        android:textStyle="bold"
        android:layout_below="@+id/estoque"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="30dp"/>

    <Button
        android:layout_width="150dp"
        android:layout_height="50dp"
        android:id="@+id/relatorio"
        android:text="@string/relatorio"
        android:textStyle="bold"
        android:layout_below="@+id/listaDeCompras"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="30dp"/>

    <Button
        android:id="@+id/buttonManterFornecedor"
        android:text="@string/buttonManterFornecedor"
        android:textStyle="bold"
        android:layout_below="@+id/relatorio"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="30dp"
        android:layout_width="150dp"
        android:layout_height="50dp" />

</RelativeLayout>



